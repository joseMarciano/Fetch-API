{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style":"off",
        "no-shadow":"off",
        "no-unused-vars": "warn",
        "eol-last": "off",
        "indent": "off",
        "comma-dangle":"off",
        "no-plusplus":"off",
        "operator-linebreak":"off",
        "max-len": "off",
        "no-lonely-if": "off"
    }

}
